    = javascript_pack_tag 'application', 'data-turbolinks-track': 'reload'
    = javascript_pack_tag 'home', 'data-turbolinks-track': 'reload'

  %body{"data-scroll-animation" => 'true'}
    = render 'layouts/header'
    = render 'layouts/banner'
