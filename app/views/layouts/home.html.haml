    %meta{:content => "width=device-width,initial-scale=1, shrink-to-fit=no", :name => "viewport"}/
    %link{rel:"icon", href:"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcR7dMvaKv9C82MdD49fhUqPOP9uazYu1cOznQ&usqp=CAU", type: "image/png"}

    = csrf_meta_tags
